@phdthesis{song2013,
  type = {{硕士}},
  title = {{构建新疆企业投资哈萨克斯坦农业风险的应对机制研究}},
  author = {宋, 洁},
  year = {2013},
  abstract = {农业作为国民经济的基础产业，其持续稳定发展是保证一国国民经济增长、人民生活水平提高的基础和前提。在经济全球化和区域经济一体化的推动下，各国农业开始走出国门，跨越国界进行生产。十七届三中全会上，党中央做出了农业``走出去''的重大战略决策，为我国农业在新形势下的发展指明了方向。哈萨克斯坦农业资源丰富，与我国农业互补、互利性强，是我国农业``走出去''的理想选择区域。我国新疆地区与哈萨克斯坦接壤，在与哈萨克斯坦开展农业合作中具有很大优势。长期以来，新疆与哈萨克斯坦在农业贸易方面取得了良好的发展，但农业投资却滞足不前。新疆对哈萨克斯坦农业投资额相对较少，投资效益不高，投资失败时有发生，这不仅是由于哈萨克斯坦国内政治、经济、法律政策等风险所导致，同时也是新疆企业内部制度、管理等方面的缺失所造成的。研究新疆企业投资哈萨克斯坦农业风险的应对机制，可以促进新疆企业内部制度、结构的优化升级，对新疆企业投资哈萨克斯坦农业过程中如何有效的应对投资风险具有借鉴意义。本文在综合国内外学者对哈萨克斯坦农业、哈萨克斯坦农业投资环境以及投资风险应对方面已有的研究基础上，通过分析新疆企业对哈萨克斯坦农业投资的现状，了解新疆企业对哈萨克斯坦农业投资的问题所在，即企业的内部风险。同时，找出新疆企业投资哈萨克斯坦农业所存在的外部风险，即哈萨克斯坦国政治、经济、法律等方面的风险。在对新疆企业所面临的内外部风险分析之后，本文将构建新疆企业投资哈萨克斯坦农业风险的应对机制。鉴于当前理论界对投资风险的研究侧重于风险的预警、防范，而对风险应对机制的研究并不多见。本文尝试从企业内部构建一套系统的、连续性的风险应对措施，并将措施具体化。最后，本文将提出几点建议来保障新疆企业风险应对机制的有效实施。},
  collaborator = {夏, 咏 and 童, 小芹},
  langid = {chinese},
  school = {新疆农业大学},
  keywords = {Kazakhstan agriculture,risk coping mechanisms,Xinjiang enterprises,哈萨克斯坦农业,新疆企业,风险应对机制},
  annotation = {5 citations(CNKI)[2021-10-17]}
}

